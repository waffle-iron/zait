{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "mocha": true
  },
  "settings": { "import/core-modules": [ "casper" ] },
  "rules": {
    "no-underscore-dangle": [2, { "allowAfterThis": true }],
    "no-restricted-syntax": [2, "WithStatement"],
    "comma-dangle": [2, "never"],
    "import/no-unresolved": 1
  }
}
